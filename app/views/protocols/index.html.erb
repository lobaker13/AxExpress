<p id="notice"><%= notice %></p>

<h1>Protocols</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Age</th>
      <th>ASA Status</th>
      <th>BCS</th>
      <th>Alert</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protocols.each do |protocol| %>
      <tr>
        <td><%= protocol.weight %></td>
        <td><%= protocol.age %></td>
        <td><%= protocol.asa %></td>
        <td><%= protocol.bcs %></td>
        <td><%= protocol.alert %></td>
        <td><%= protocol.patient %></td>
        <td><%= link_to 'Show', protocol %></td>
        <td><%= link_to 'Edit', edit_protocol_path(protocol) %></td>
        <td><%= link_to 'Destroy', protocol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Protocol', new_protocol_path %>
